                <block class="School\Creation\Block\Adminhtml\Faq\Edit\Tab\Content" name="creation_faq_edit_tab_content"/>
                <block class="School\Creation\Block\Adminhtml\Faq\Edit\Tab\SearchEngineOptimisation" name="creation_faq_edit_tab_optimisation"/>
                <block class="School\Creation\Block\Adminhtml\Faq\Edit\Tab\Websites" name="creation_faq_edit_tab_websites"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">general_section</argument>
                    <argument name="block" xsi:type="string">creation_faq_edit_tab_general</argument>
                </action>
               
            </block>
        </referenceContainer>
    </body>
</page>
